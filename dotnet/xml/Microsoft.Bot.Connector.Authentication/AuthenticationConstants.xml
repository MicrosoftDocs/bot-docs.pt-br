    <Member MemberName="AudienceClaim">
      <MemberSignature Language="C#" Value="public const string AudienceClaim;" />
      <MemberSignature Language="ILAsm" Value=".field public static literal string AudienceClaim" />
      <MemberSignature Language="DocId" Value="F:Microsoft.Bot.Connector.Authentication.AuthenticationConstants.AudienceClaim" />
